/*
	 // $1, $2 for tab stops, ${id} and ${id:label} and ${1:label} for variables. Variables with the same id are connected.
*/
{
	"HTML comment": {
		"prefix": "htmlcomment",
		"body": [
			"<!-- ${comment} \n==================================== -->"
		],
		"description": "A PHP comment"
	},
	"PHP comment": {
		"prefix": "phpcomment",
		"body": [
			"/*\n	 ${comment} \n*/"
		],
		"description": "A PHP comment"
	}
}
